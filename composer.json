{
    "name": "gealex/doublit",
    "description": "Doublit : Double and test PHP classes easily in PhpUnit",
    "keywords": ["test", "testing", "test-doubles", "mock", "phpunit", "unit-testing"],
    "homepage": "https://github.com/gealex/doublit",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandre GEISWILLER",
            "email": "alexandre.geiswiller@gmail.com"
        }
    ],
    "require": {
        "php": ">=7",
        "phpunit/phpunit": ">=6"
    },
    "autoload": {
        "psr-4": {
            "Doublit\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    }
}
