{
    "name": "gealex/doublit",
    "description": "Doublit : A simple tool to test your class dependencies in PhpUnit",
    "keywords": ["php", "test", "testing", "doubles", "mock", "phpunit", "unit-testing"],
    "homepage": "https://github.com/gealex/doublit",
    "license": "MIT",
    "authors": [
        {
            "name": "Alexandre GEISWILLER",
            "email": "alexandre.geiswiller@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0|~7.0|~8.0"
    },
    "autoload": {
        "psr-4": {
            "Doublit\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    }
}
